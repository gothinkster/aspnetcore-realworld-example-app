    "@testing-library/user-event": "^7.2.1",
    "@types/axios": "^0.14.0",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.20.18",
    "@types/ramda": "^0.27.44",
    "@types/react": "^16.14.11",
    "@types/react-dom": "^16.9.14",
    "@types/react-redux": "^7.1.18",
